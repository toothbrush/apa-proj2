imports
{
import CCO.SourcePos
import CCO.Types
}

-------------------------------------------------------------------------------
-- Syntax
-------------------------------------------------------------------------------

DATA Tm
  | Tm  pos : {SourcePos}  t : Tm_

DATA Tm_
  | Var  x : {Var}
  | Lam  x : {Var}  t1 : Tm
  | App  t1 : Tm  t2 : Tm
  | Let  x : {Var}  t1 : Tm  t2 : Tm
